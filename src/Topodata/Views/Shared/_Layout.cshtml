<!DOCTYPE html>
<html>
<head>
    <title>@ViewData["Title"] - Topodata</title>

    <!-- Meta -->
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="" name="description">
    <meta content="" name="author">

    <!-- Favicon -->
    <link rel="shortcut icon" href="~/favicon.ico">

    <!-- Web Fonts -->
    <link rel="shortcut" type="text/css" href="//fonts.googleapis.com/css?family=Open+Sans:400,300,600&subset=cyrillic,latin">

    <!-- CSS Global Compulsory -->
    <link rel="stylesheet" href="~/plugins/bootstrap/css/bootstrap-rtl.min.css">
    <link rel="stylesheet" href="~/css/style.css">

    <!-- CSS Header and Footer -->
    <link rel="stylesheet" href="~/css/headers/header-default.css">
    <link rel="stylesheet" href="~/css/footers/footer-default.css">

    <!-- CSS Implementing Plugins -->
    <link href="~/plugins/animate.css" rel="stylesheet"/>
    <link href="~/plugins/line-icons/line-icons.css" rel="stylesheet"/>
    <link href="~/plugins/font-awesome/css/font-awesome.css" rel="stylesheet"/>

    <!-- CSS Theme -->
    <link href="~/css/theme-colors/default.css" rel="stylesheet"/>


    <!-- CSS Customization -->
    <link href="~/css/custom.css" rel="stylesheet"/>
</head>
<body>
<div class="wrapper">
    <!--=== Header ===-->
    <div class="header">
        <!-- Topbar -->
        <div class="topbar">


        </div>
        <!-- End Topbar -->
        <!-- Navbar -->
        <div class="navbar navbar-default mega-menu" role="navigation">


        </div>
        <!-- End Navbar -->
    </div>
    <!--=== End Header ===-->

    <!--=== Content Part ===-->
    <div class="container content">
        @RenderBody()
    </div><!--/container-->
    <!--=== End Content Part ===-->

    <!--=== Default Footer ===-->
    <div class="footer-default">
        <div class="footer">
            <div class="container">

            </div>
        </div><!--/footer-->

        <div class="copyright">
            <div class="container">

            </div>
        </div><!--/copyright-->
    </div>
    <!--=== End Default Footer ===-->

    @*    @RenderSection("scripts", required: false)*@
</div>
<!-- JS Global Compulsory -->
<script src="~/wwwroot/plugins/jquery/jquery.min.js"></script>
<script src="~/wwwroot/plugins/jquery/jquery-migrate.min.js"></script>
<script src="~/wwwroot/plugins/bootstrap/js/bootstrap.min.js"></script>

<!-- JS Implementing Plugins -->
<script src="~/wwwroot/plugins/back-to-top.js"></script>
<script src="~/wwwroot/plugins/smoothScroll.js"></script>

<!-- JS Customization -->
<script type="text/javascript" src="~/wwwroot/js/custom.js"></script>

<!-- JS Page Level -->
<script src="~/wwwroot/js/app.js"></script>
<script>
    jQuery(document).ready(function() {
        App.init();
    });
</script>

<!--[if lt IE 9]>
    <script src="~/wwwroot/plugins/respond.js"></script>
    <script src="~/wwwroot/plugins/html5shiv.js"></script>
    <script src="~/wwwroot/plugins/placeholder-IE-fixes.js"></script>
<![endif]-->

</body>
</html>